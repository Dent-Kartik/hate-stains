    <!-- --- Discount Coupon Issue Fix Start --- -->
    <script>
    // Apply discount if available on checkout page
    //console.log('Applying Coupon.');
    setTimeout(function() {
        if (window.location.href.indexOf('/checkouts/') != -1) {
            //console.log('On checkout page.')
            // Check if the cookie has a dicount code
            var disCookie, startPos, endPos, disCoupon, tags;
            disCookie = document.cookie;
            startPos = disCookie.indexOf('discount_code=');
            // Cookie exists
            if (startPos != -1) {
                endPos = disCookie.indexOf(';', startPos);
                disCoupon = disCookie.slice(startPos + 14, endPos);
                //console.log('Discount coupon detected: ' + disCookie);

                // Check if any coupon code is already applied, if not then apply it
                tags = document.querySelectorAll('div.tags-list div.tag form span.reduction-code__text');
                //console.log('Some Coupon already exists.')
                if (tags.length > 0) {
                    //console.log('Already applied coupons: ' + tags)
                    tags.forEach((coupon) => {
                        if (coupon.innerText.toLowerCase() == disCoupon) {
                            //console.log('Coupon already exists.');
                            return false;
                        }
                    });
                } else {
                    // Apply the discount code
                    //console.log('Applying coupon: ' + disCoupon);
                    document.getElementById('checkout_reduction_code').value = disCoupon;
                    document.querySelector('div.field--show-floating-label button[type="submit"]').click();
                }
            }
        }
    }, 100);
    //console.log('Applied Coupon...'); 
    </script>
    <!-- --- Discount Coupon Issue Fix End --- -->
    
